{
    "name": "New Tab",
    "description": "New Tab Page",
    "version": "1.0.0",
    "manifest_version": 3,
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "bookmarks",
        "history",
        "topSites",
        "contextMenus",
